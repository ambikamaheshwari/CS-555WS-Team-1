	public static void IndividualCount() throws IOException, GedcomParserException {
		BufferedReader br;
	
		while ((line = br .readLine()) != null) {
			if (line.contains("@I"))
				{
				String indi = line.trim().substring(4, 5);
			
				}
	}
	}
